<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict/>
	<key>Label</key>
	<string>EtsyThingsBridge</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Path/to/EtsyThingsBridge</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/EtsyThingsBridge.err</string>
	<key>StandardOutPath</key>
	<string>/tmp/EtsyThingsBridge.out</string>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>0</integer>
	</dict>
</dict>
</plist>
